var 
a, b : integer;

begin
WriteLn('Enter number 1:');
ReadLn(a);

WriteLn('Enter number 2:');
ReadLn(b);

WriteLn('---------------------------------');

WriteLn('Output sum:');
WriteLn(a + b);

WriteLn('Output difference:');
WriteLn(a - b);

WriteLn('Output multiplication:');
WriteLn(a * b);

WriteLn('Output division:');
WriteLn(a / b);
end.